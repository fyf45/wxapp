@import '/template/productList-template/productList-template.wxss';

.container {
  width: 100%;
  background: #fafafa;
  min-height: 100%;
  overflow: hidden;
}

.container_overflow {
  width: 100%;
  background: #fafafa;
  height: 100vh;
  overflow: hidden;
}

.searchList {
  padding: 24rpx 24rpx 0 24rpx;
}

.program-list {
  width: 100%;
  height: 100%;
  padding: 0 0 24rpx 0;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
}

.my-tab {
  width: 100%;
  display: flex;
  padding: 24rpx;
  justify-content: space-between;
}

.my-tab .tab-item {
  width: 338rpx;
  height: 168rpx;
  background-image: linear-gradient(-80deg, rgba(238, 67, 105, 0.96) 0%, rgba(253, 121, 161, 0.96) 100%, rgba(254, 188, 208, 0.96) 100%, rgba(255, 255, 255, 0.96) 100%),
                      linear-gradient(#f87098, #f87098);
  background-blend-mode: normal, normal;
  border-radius: 10rpx;
  padding: 35rpx 0;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.my-tab .tab-item:nth-child(1) .tab-img {
  width: 47rpx;
  height: 49rpx;
  display: block;
}

.my-tab .tab-item:nth-child(2) .tab-img {
  width: 41rpx;
  height: 56rpx;
  display: block;
}

.my-tab .tab-item:nth-child(1) .tab-text {
  font-size: 24rpx;
  color: #fff;
}

.my-tab .tab-item:nth-child(2) .tab-text {
  font-size: 24rpx;
  color: #fff;
}

.therapy-infoBox {
  padding: 0 24rpx;
}

.therapy-info {
  width: 100%;
  height: 128rpx;
  background-color: #fff;
  box-shadow: 0rpx 3rpx 29rpx 0rpx #e3e5e7;
  border-radius: 10rpx;
  display: flex;
  justify-content: space-around;
  text-align: center;
}

.info-item {
  width: 33.33%;
  height: 100%;
  padding: 24rpx 0;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: space-between;
}

.therapy-info .info-title {
  font-size: 30rpx;
  color: #ef486e;
}

.therapy-info .info-num {
  font-size: 24rpx;
  color: #ababab;
}

.my-therapy {
  width: 100%;
  padding-top: 30rpx;
  position: relative;
}

.my-program {
  width: 100%;
  padding: 0rpx 24rpx 0 24rpx;
}

.my-therapy .therapy-title {
  padding-left: 24rpx;
  padding-bottom: 24rpx;
  font-size: 30rpx;
  color: rgba(53, 53, 53, 0.8);
}

.my-program .program-title {
  padding-top: 30rpx;
  border-top: 1px solid #f8f8f8;
}

.my-therapy .therapy-list {
  width: 690rpx;
  height: 464rpx !important;
  margin: auto;
}

.therapy-itemBox {
  height: 232rpx !important;
}

.my-therapy .therapy-item {
  width: 100%;
  height: 208rpx !important;
  padding: 24rpx;
  display: -webkit-flex;
  flex-flow: row nowrap;
  align-items: center;
  background-color: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 3rpx 29rpx 0rpx rgba(227, 229, 231, 1);
  border-radius: 5px;
}

.therapy-cont {
  flex: 1;
  height: 100%;
  margin-left: 24rpx;
  display: -webkit-flex;
  flex-direction: column;
  position: relative;
}

.therapy-cont-typeBox {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
}

.therapy-cont-title {
  font-size: 28rpx;
  color: rgba(53, 53, 53, 1);
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  margin-bottom: 8rpx;
}

.therapy-cont-txt {
  font-size: 24rpx;
  text-align: justify;
  color: rgba(144, 144, 144, 1);
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.therapy-cont-type {
  width: 100%;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}

.therapy-type-left {
  font-size: 24rpx;
  color: rgba(190, 190, 190, 1);
}

.therapy-type-right {
  flex: 1;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-end;
  align-items: center;
}

.therapy-supperBox, .therapy-commentBox {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}

.therapy-supperBox {
  margin-right: 24rpx;
}

.therapy-supperBox-img, .therapy-commentBox-img, .therapy-shareImg {
  width: 24rpx;
  height: 24rpx;
}

.therapy-supperBox-txt, .therapy-commentBox-txt {
  font-size: 26rpx;
  color: rgba(53, 53, 53, 0.6);
  padding-left: 12rpx;
}

.my-therapy .therapy-img {
  width: 209rpx;
  height: 158rpx;
  border-radius: 5px;
}

/* 自定义轮播dots */

.dots {
  width: 8rpx;
  height: 200rpx;
  background-color: #d9d9d9;
  border-radius: 4rpx;
  position: absolute;
  top: 40%;
  right: 24rpx;
  display: flex;
  flex-direction: column;
}

.dots .dot {
  width: 8rpx;
  height: 200rpx;
  border-radius: 4rpx;
  transition: all 0.1s linear;
}

.dots .active {
  background-color: #ef486e;
}

/* 用户指引 */

.firstGuide {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  background: rgba(0, 0, 0, 0.72);
}

.firstGuide_container {
  width: 100%;
  height: 100%;
  position: relative;
}

.firstGuide_contentBox {
  width: 581rpx;
  height: 246rpx;
  border-radius: 15px;
  border: solid 2rpx rgba(255, 255, 255, 1);
  padding: 40rpx;
  position: absolute;
  left: 50%;
  transform: translate(-50%, 0);
  top: 425rpx;
}
.firstGuide_boxDiagnosis{
  top: 425rpx;
}
.firstGuide_boxConditioning{
  top: 425rpx;
}
.firstGuide_boxSearch{
  top: 235rpx;
}
.firstGuide_boxMoxa{
  top: 58%;
}
.firstGuide_content {
  width: 100%;
  height: 100%;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.firstGuide_txtBox {
  display: -webkit-flex;
  flex-direction: column;
}
.fixedBottom{
  width: 100%;
  height: 99rpx;
  position: fixed;
  bottom: 0;
  left: 0
}
.fixedBottom-url{
  height: 100%;
}
.fixedBottom_img{
  display: block;
  width: 100%;
  height: 100%;
}
/* button样式重置 */

button {
  margin: 0;
  padding: 0;
  background: none;
  line-height: 1;
}

button::after {
  border: none;
}

.firstGuide_txt {
  font-size: 26rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}

.firstGuide_btn {
  margin-top: 20rpx;
  color: rgba(255, 255, 255, 1);
  width: 215rpx;
  height: 72rpx;
  background: url('data:image/png;base64,');
  background-size: cover;
  line-height: 72rpx;
  text-align: center;
  font-size: 24rpx;
}

.firstGuide_arrow {
  width: 59rpx;
  height: 105rpx;
  position: absolute;
  top: 300rpx;
  left: 170rpx;
}

.firstGuide_arrowDiagnosis {
  top: 300rpx;
  left: 170rpx;
}

.firstGuide_arrowConditioning {
  top: 300rpx;
  left: 530rpx;
}

.firstGuide_arrowSearch {
  top: 110rpx;
  left: 170rpx;
}

.firstGuide_arrowMoxa {
  top: 80%;
  left: 50%;
  transform: translate(-50%,0)
}

.firstGuide_arrow_img {
  display: block;
  width: 100%;
  height: 100%;
}

.display {
  position: relative;
  z-index: 1000;
}
