.crustaInfo {
  width: 100%;
  min-height: 100%;
  background: #fafafa;
  position: relative;
}

/* 资讯头部 */

.crustaInfo-head {
}

.crustaInfo-headTitle {
  font-size: 38rpx;
  color: rgba(0, 0, 0, 0.8);
  padding: 30rpx 24rpx 0;
  text-align: justify;
}

.headInfo {
  width: 100%;
  display: -webkit-flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  padding: 39rpx 24rpx 0;
  padding-bottom: 12rpx;
  border-bottom: 1rpx solid rgba(212, 212, 212, 0.6);
}

.headInfo-left {
  display: -webkit-flex;
  flex-flow: row nowrap;
}

.headInfo-leftImgBox {
  width: 42rpx;
  height: 42rpx;
  background-color: rgba(239, 239, 239, 0.8);
  border-radius: 50%;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}

.headInfo-left-logo {
  display: block;
  width: 21rpx;
  height: 28rpx;
}

.headInfo-left-txt {
  font-size: 28rpx;
  color: rgba(0, 0, 0, 0.6);
  padding-left: 12rpx;
}

.headInfo-right {
  font-size: 28rpx;
  color: rgba(140, 140, 140, 0.8);
}

/* 资讯内容 */

.crustaInfo-content {
  padding: 40rpx 30rpx;
  border-bottom: 1rpx solid rgba(222, 222, 222, 0.6);
}

.crustaInfo-content-txt {
  display: block;
  font-size: 28rpx;
  color: rgba(38, 38, 40, 0.6);
  text-indent: 50rpx;
  text-align: justify;
  line-height: 48rpx;
  padding-bottom: 30rpx;
}

.crustaInfo-content-image {
  width: 690rpx;
  background-color: rgba(0, 0, 0, 1);
  margin:0 auto 30rpx;
}
.indent_txt{
  text-indent: 0
}
.crustaInfo-content-complex {
  padding-top: 30rpx;
  display: -webkit-flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}

.complex-left {
  font-size: 28rpx;
  color: rgba(144, 144, 144, 0.6);
}

.complex-right, .reviewHead-right {
  display: -webkit-flex;
  flex-flow: row nowrap;
  align-items: center;
}

.complex-right-icon {
  width: 24rpx;
  height: 24rpx;
}

.complex-right-num {
  font-size: 28rpx;
  color: rgba(53, 53, 53, 0.6);
  padding-left: 12rpx;
}

/* 资讯评论 */

.crustaInfo-review {
  padding: 24rpx;
}

.crustaInfo-reviewHead {
  display: -webkit-flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  padding-bottom: 40rpx;
}

.reviewHead-left {
  font-size: 30rpx;
  color: rgba(53, 53, 53, 0.6);
}
.reviewHead-left>text{
  font-size: 34rpx;
}
.reviewHead-right-icon {
  display: block;
  width: 26rpx;
  height: 21rpx;
}

.reviewHead-right-txt {
  font-size: 28rpx;
  color: rgba(239, 72, 110, 0.6);
  padding: 0 0 0 12rpx;
  margin: 0;
  font-weight: bold;
  line-height: 1;
}

.reviewHead-right-txt::after, .crustaInfo-send::after {
  border: 0;
  background: 0;
}

.crustaInfo-reviewCon {
  display: -webkit-flex;
  flex-flow: row nowrap;
  padding-bottom: 48rpx;
}

.reviewCon-left {
  display: -webkit-flex;
  flex-flow: row nowrap;
}

.reviewCon-leftImg {
  display: block;
  width: 68rpx;
  height: 68rpx;
  background-color: rgba(239, 122, 122, 1);
  border-radius: 50%;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}

.reviewCon-type {
  display: -webkit-flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 16rpx;
}

.reviewCon-type-rate {
  display: -webkit-flex;
  flex-flow: row nowrap;
  align-items: center;
}

.reviewCon-type-name {
  width: 526rpx;
  font-size: 24rpx;
  color: rgba(53, 53, 53, 0.6);
  word-break: break-all;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.reviewCon-type-content {
  display: block;
  width: 526rpx;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.6);
  padding-bottom: 14rpx;
  text-align: justify;
  word-break: break-all
}

.reviewCon-type-time {
  font-size: 24rpx;
  color: rgba(144, 144, 144, 0.6);
}

.reviewCon-right {
  flex: 1;
  display: -webkit-flex;
  flex-direction: column;
  padding-left: 20rpx;
}

.reviewCon-right-img {
  display: block;
  width: 21rpx;
  height: 24rpx;
}

.reviewCon-right-txt {
  font-size: 24rpx;
  color: rgba(53, 53, 53, 0.6);
  padding-left: 12rpx;
}

/* 资讯悬浮图标 */

.crustaInfo-flow {
  position: fixed;
  width: 100%;
  height: 100%;
  pointer-events: none;
  left: 0;
  top: 0
}
.crustaInfo-flowBox{
  pointer-events: auto;
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
}
.crustaInfo-flowIcon {
  display: block;
  width: 100%;
  height: 100%;
  background-image: url('data:image/png;base64,');
  background-position: center;
  background-size: cover;
  background-color: transparent!important;
}
.crustaInfo-flowIcon::after{
  border: 0;
}
/* 发送评论框 */

.crustaInfo-flexBox {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.2);
  z-index: 10;
}

.crustaInfo-conBox {
  width: 100%;
  height: 223rpx;
  background-color: rgba(255, 255, 255, 1);
  z-index: 100;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 16rpx 0rpx 16rpx 24rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}

.crustaInfo-con {
  width: 100%;
  display: -webkit-flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}

.crustaInfo-message {
  width: 604rpx;
  height: 172rpx;
  border-radius: 10rpx;
  border: solid 1rpx rgba(170, 170, 170, 0.4);
  padding: 24rpx 0 0 24rpx;
}

.crustaInfo-send {
  flex: 1;
  padding: 0;
  background: rgba(255, 255, 255, 1);
  font-size: 34rpx;
  color: rgba(239, 72, 110, 0.6);
  line-height: 1;
  margin: 30rpx 0 0 0rpx;
  text-align: center;
}

.crustaInfo-send[disabled] {
  flex: 1;
  padding: 0;
  background: rgba(255, 255, 255, 1) !important;
  font-size: 34rpx;
  color: rgba(201, 199, 199, 0.753) !important;
  line-height: 1;
  margin: 30rpx 0 0 0rpx;
  text-align: center;
}

.crustaInfo-mesSstle {
  font-size: 28rpx;
  color: rgba(144, 144, 144, 0.6);
}

.complex-right-numActive {
  color: rgba(239, 72, 110, 1);
}

