.historyConvert{
  width: 100%;
  height: 100%;
  padding: 24rpx;
  background: #fafafa;
}
.historyConvert-list{
  display: flex;
  flex-direction: column;
}
.historyConvert-item{
  height: 112rpx;
	background-color: #fffefe;
	box-shadow: 0rpx 2rpx 29rpx 0rpx 
		#e1e0e0;
	border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 46rpx 0 38rpx;
  margin-bottom: 24rpx;
}
.historyConvert-item-leftText{
  font-size: 28rpx;
	color: #353535;
}
.historyConvert-item-rightBox{
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  padding: 10rpx 20rpx;
	height: 45rpx;
	background-color: #ef486e;
	border-radius: 23rpx;
}
.historyConvert-item-rightBox-minus{
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  padding: 10rpx 20rpx;
	height: 45rpx;
	background-color: #1dd1a1;
	border-radius: 23rpx;
}
.historyConvert-item-rightImg{
  width: 26rpx;
	height: 26rpx;
}
.historyConvert-item-rightText{
  font-size: 24rpx;
	color: #ffffff;
  padding-left: 8rpx;
}
.commonNullContainer-img {
  display: block;
  width: 129rpx;
	height: 98rpx;
}
.down_prompt{
  position: relative;
  text-align: center;
} 
.down_prompt_txt{
  font-size: 24rpx;
	color: rgba(204, 204, 204, 1);
}
.down_prompt_txt::before,.down_prompt_txt::after{
  display: block;
  content: "";
  width: 52rpx;
	height: 0rpx;
  border-bottom: 1rpx solid rgba(204, 204, 204, .6);
  position: absolute;
  top: 50%;
}
.down_prompt_txt::before{
  left: 30%;
}
.down_prompt_txt::after{
  left: 62%;
}